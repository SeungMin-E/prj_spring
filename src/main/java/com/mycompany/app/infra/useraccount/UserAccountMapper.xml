<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

    <mapper namespace = "com.mycompany.app.infra.useraccount.UserAccountMapper">
    
    <resultMap type="resultMapObj" id="com.mycompany.app.infra.useraccount" />
    
    	<sql id="user_val">
    		seq
    		, userID
    		, userPW
    		, userName
    		, userAge
    		, gender
    		, userStatus
    	</sql>
    	
    	<sql id="user_db">
    		FROM
    			userAccount
    		WHERE 1=1 
    			AND userName LIKE CONCAT('%',#{},'%') 
    	</sql>
    	
    	
    
    </mapper>