<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
    <!-- 전체 매퍼중에서 유일한 값을 사용해야한다. 해서 관계적으로 해당 파일의 패스와 파일이름(확장제외) 그리고 dao에서 namespace와 일치해야한다. -->
<mapper namespace="com.mycompany.app.codegroup.CodeGroupMapper">
				     		
				     		<!-- select 결과를 받을 객체 -->		   
    <resultMap id="resultMapObj" type="com.mycompany.app.codegroup.CodeGroup"></resultMap>
    
	<select id="selectList" resultMap="resultMapObj">
		select
			 seq,
			 gender
		 from codeGroup
	</select>
	
	    
</mapper>    